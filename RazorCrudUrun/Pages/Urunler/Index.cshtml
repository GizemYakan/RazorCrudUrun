@page@model RazorCrudUrun.Pages.Urunler.IndexModel@{ }<a asp-page="/Urunler/Ekle">Yeni Ürün Ekle</a><form method="post">    <table class="table">        <thead>            <tr>                <th>Ürün Adı</th>                <th>Ürün Fiyatı</th>                <th>İşlemler</th>            </tr>        </thead>        <tbody>            @foreach (var item in Model._db.Urunler)            {<tr>    <td>@item.UrunAd</td>    <td>@item.BirimFiyat</td>    <td>        <a class="btn btn-warning" asp-page="/Urunler/Duzenle" asp-route-id="@item.Id">Düzenle</a>        <button class="btn btn-danger" type="submit" asp-page-handler="delete" asp-route-id="@item.Id">            Sil        </button>    </td></tr>}        </tbody>    </table></form>